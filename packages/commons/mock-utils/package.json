{
  "name": "@fern-api/mock-utils",
  "version": "0.0.1",
  "description": "Fern utils for generated WireMock testing stubs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@fern-fern/ir-sdk": "^61.7.0",
    "@fern-api/base-generator": "workspace:*",
    "@fern-api/ir-utils": "workspace:*",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.19.0",
    "@types/uuid": "^9.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.test.ts", "**/?(*.)+(spec|test).ts"]
  }
}
