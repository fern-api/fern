    "es-toolkit": "^1.32.0",
    "esbuild": "^0.25.0",
    "execa": "^9.5.1",
    "tmp-promise": "^3.0.3",
    "tsup": "^8.5.0",
    "typescript": "5.9.3",
    "vitest": "^4.0.8",
