{
  "name": "@fern-typescript/testing-utils",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "tsc --build --clean tsconfig.json && tsc --build --clean tsconfig.cjs.json",
    "compile": "tsc --build tsconfig.json && tsc --build tsconfig.cjs.json && echo '{ \"type\": \"commonjs\" }' > dist/cjs/package.json && echo '{ \"type\": \"module\" }' > dist/esm/package.json",
    "test": "jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore",
    "lint:eslint:fix": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@fern-api/api": "workspace:*",
    "@fern-api/commons": "workspace:*",
    "@fern-api/compiler": "workspace:*",
    "@fern-typescript/commons": "workspace:*",
    "execa": "^5.1.1",
    "fern-api": "workspace:*",
    "memfs": "^3.4.1",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.33",
    "depcheck": "^1.4.3",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2"
  }
}
