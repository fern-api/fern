{
  "name": "@fern-typescript/zurg",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern-typescript.git",
    "directory": "packages/core-utilities/zurg"
  },
  "files": [
    "lib"
  ],
