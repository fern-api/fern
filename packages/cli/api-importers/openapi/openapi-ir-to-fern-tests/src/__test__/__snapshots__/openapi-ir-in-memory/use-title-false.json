          }
        ]
      },
      "UnionJob_Job<TlInference>": {
        "allOf": [
          {
            "type": "object",
            "required": [
              "type"
            ],
            "properties": {
              "type": {
                "type": "string",
                "example": "TL_INFERENCE"
              }
            }
          },
          {
            "$ref": "#/components/schemas/Job<TlInference>"
