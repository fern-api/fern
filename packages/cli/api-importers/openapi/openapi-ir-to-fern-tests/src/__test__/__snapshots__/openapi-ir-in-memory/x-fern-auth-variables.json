    "info": {
      "title": "Test extension `x-fern-parameter-name` for parameters.",
      "version": "1.0.0"
    },
    "paths": {
      "/user": {
        "get": {
          "x-fern-sdk-group-name": [
            "user"
          ],
          "x-fern-sdk-method-name": [
            "get"
          ],
          "responses": {
            "200": {
              "description": "Success",
