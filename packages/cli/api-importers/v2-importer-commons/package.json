{
  "name": "@fern-api/v2-importer-commons",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "compile:debug": "tsc --build --sourceMap",
    "test": "vitest --run --passWithNoTests",
    "test:debug": "pnpm run test --inspect-brk --no-file-parallelism",
    "test:update": "vitest --run -u --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-pattern=../../../.eslintignore",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@fern-api/api-workspace-commons": "workspace:*",
    "@fern-api/casings-generator": "workspace:*",
    "@fern-api/cli-logger": "workspace:*",
    "@fern-api/configuration": "workspace:*",
    "@fern-api/core-utils": "workspace:*",
    "@fern-api/fern-definition-schema": "workspace:*",
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/ir-sdk": "workspace:*",
    "@fern-api/ir-utils": "workspace:*",
    "@fern-api/logger": "workspace:*",
    "js-yaml": "^4.1.0",
    "js-yaml-source-map": "^0.2.2",
    "lodash-es": "^4.17.21",
    "openapi-types": "^12.1.3",
    "yaml": "^2.4.5",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@fern-api/configs": "workspace:*",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/js-yaml": "^4.0.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "18.15.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.2",
    "vitest": "^2.1.9"
  }
}