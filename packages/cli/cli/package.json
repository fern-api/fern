    "ansi-escapes": "^5.0.0",
    "axios": "^1.12.0",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "depcheck": "^1.4.7",
    "get-port": "^7.1.0",
    "immer": "^10.1.1",
