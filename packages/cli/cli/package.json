    "js-yaml": "^4.1.0",
    "latest-version": "^9.0.0",
    "lodash-es": "^4.17.21",
    "openapi-types": "^12.1.3",
    "ora": "^7.0.1",
    "semver": "^7.6.2",
    "tar": "^6.2.1",
    "tmp-promise": "^3.0.3",
    "tsup": "^8.5.0",
    "typescript": "5.9.3",
    "undici": "^6.21.1",
    "url-join": "^5.0.0",
    "validate-npm-package-name": "^5.0.1",
    "vitest": "^4.0.8",
    "yaml": "^2.4.5",
    "yargs": "^17.4.1"
  }
}
