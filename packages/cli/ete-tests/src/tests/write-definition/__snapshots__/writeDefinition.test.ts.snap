        type: integer
      exp_month:
        docs: Two-digit number representing the card's expiration month.
        type: long
      exp_year:
        docs: Four-digit number representing the card's expiration year.
        type: long
      name:
        docs: Cardholder's full name as it appears on the card.
        type: string
