      openapi: openapi/trains/openapi.yml
  Trip:
    properties:
      arrival_time:
        docs: The date and time when the trip arrives
        type: optional<datetime>
      bicycles_allowed:
        docs: Indicates whether bicycles are allowed on the trip
        type: optional<boolean>
