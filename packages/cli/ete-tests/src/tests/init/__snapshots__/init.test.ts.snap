    properties:
      id: optional<string>
      event_type: optional<string>
      occurred_at: optional<string>
      _links: optional<_InlinedType36>
  _InlinedType35:
    docs: Link to view the full event payload.
    properties:
      href: optional<string>
  _InlinedType36:
    properties:
      payload: optional<_InlinedType35>
  Integration:
    docs: >-
      Represents a connection between a client application and Covie's systems
