  _InlinedType40:
    properties:
      agency_name: optional<string>
      agent_name: optional<string>
      agent_email: optional<string>
      image_urls: optional<_InlinedType39>
  _InlinedType41:
    properties:
      brand: optional<_InlinedType38>
      display_name: optional<string>
      use_name_as_display_name: optional<boolean>
      hosted: optional<_InlinedType40>
  _InlinedType42:
    properties:
      primary: optional<string>
  _InlinedType43:
    docs: Branding configuration for the embed.
    properties:
      logo: optional<string>
      hide_logo: optional<boolean>
      colors: optional<_InlinedType42>
  _InlinedType44:
    properties:
      agency_logo: optional<string>
      agency_photo: optional<string>
  _InlinedType45:
    properties:
      agency_name: optional<string>
      agent_name: optional<string>
