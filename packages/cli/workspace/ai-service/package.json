{
  "name": "@fern-api/ai-service",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "packages/cli/workspace/ai-service"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "source": "./src/index.ts",
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "main": "lib/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": ["lib", "baml_client"],
  "scripts": {
    "clean": "rm -rf ./lib ./baml_client && tsc --build --clean",
    "baml-generate": "baml-cli generate",
    "prebuild": "pnpm run baml-generate",
    "compile": "pnpm run baml-generate && tsc --build",
    "compile:debug": "pnpm run baml-generate && tsc --build --sourceMap",
    "depcheck": "depcheck",
    "test": "vitest --run --passWithNoTests",
    "test:debug": "pnpm run test --inspect --no-file-parallelism",
    "test:update": "vitest --passWithNoTests --run -u"
  },
  "dependencies": {
    "@fern-api/core-utils": "workspace:*",
    "@fern-api/task-context": "workspace:*",
    "@boundaryml/baml": "^0.62.0"
  },
  "devDependencies": {
    "@fern-api/configs": "workspace:*",
    "@types/node": "18.15.3",
    "depcheck": "^1.4.7",
    "typescript": "5.9.3",
    "vitest": "^4.0.8"
  }
}
