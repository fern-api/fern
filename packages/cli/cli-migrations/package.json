{
  "name": "@fern-api/cli-migrations",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "packages/cli/yaml/yaml-migrations"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "source": "./src/index.ts",
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "main": "lib/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": ["lib"],
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "compile:debug": "tsc --build --sourceMap",
    "depcheck": "depcheck",
    "test": "vitest --run",
    "test:debug": "pnpm run test --inspect --no-file-parallelism",
    "test:update": "vitest --run -u"
  },
  "dependencies": {
    "@fern-api/configuration-loader": "workspace:*",
    "@fern-api/core-utils": "workspace:*",
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/semver-utils": "workspace:*",
    "@fern-api/task-context": "workspace:*",
    "@fern-fern/docs-config": "0.0.80",
    "@fern-fern/legacy-docs-config": "0.0.58",
    "chalk": "^5.3.0",
    "find-up": "^6.3.0",
    "glob": "^11.0.0",
    "inquirer": "^9.2.23",
    "js-yaml": "^4.1.0",
    "yaml": "^2.4.5",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@fern-api/configs": "workspace:*",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.8",
    "@types/node": "18.15.3",
    "depcheck": "^1.4.7",
    "tmp-promise": "^3.0.3",
    "typescript": "5.9.3",
    "vitest": "^4.0.8"
  }
}
