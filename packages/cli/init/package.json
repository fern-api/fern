{
  "name": "@fern-api/init",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "packages/cli/init"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "source": "./src/index.ts",
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "main": "lib/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "compile:debug": "tsc --build --sourceMap",
    "depcheck": "depcheck",
    "test": "vitest --passWithNoTests --run",
    "test:debug": "pnpm run test --inspect --no-file-parallelism",
    "test:update": "vitest --passWithNoTests --run -u"
  },
  "dependencies": {
    "@fern-api/auth": "workspace:*",
    "@fern-api/configuration-loader": "workspace:*",
    "@fern-api/core": "workspace:*",
    "@fern-api/fern-definition-formatter": "workspace:*",
    "@fern-api/fern-definition-schema": "workspace:*",
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/login": "workspace:*",
    "@fern-api/mintlify-importer": "workspace:*",
    "@fern-api/readme-importer": "workspace:*",
    "@fern-api/task-context": "workspace:*",
    "axios": "^1.8.2",
    "chalk": "^5.3.0",
    "fs-extra": "^11.1.1",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@fern-api/configs": "workspace:*",
    "@types/fs-extra": "^11.0.1",
    "@types/js-yaml": "^4.0.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "18.15.3",
    "depcheck": "^1.4.7",
    "typescript": "5.7.2",
    "vitest": "^2.1.9"
  }
}