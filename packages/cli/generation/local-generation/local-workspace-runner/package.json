    "@fern-api/sdk": "0.12.3",
    "chalk": "^5.3.0",
    "decompress": "^4.2.1",
    "tmp-promise": "^3.0.3",
    "url-join": "^5.0.0",
    "prettier2": "npm:prettier@^2.7.1"
  },
  "devDependencies": {
    "@types/decompress": "^4.2.7",
    "@types/jest": "^29.5.14",
    "@types/node": "18.15.3",
    "@types/tmp": "^0.2.4",
    "@types/url-join": "4.0.1",
    "@types/prettier2": "npm:@types/prettier@^2.7.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.16.0",
    "vitest": "^2.1.8",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "prettier": "^3.4.2",
    "typescript": "5.7.2"
  }
