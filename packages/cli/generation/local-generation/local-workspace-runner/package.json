{
  "name": "@fern-api/local-workspace-runner",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "packages/cli/generation/local-generation/local-workspace-runner"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "yarn compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../../../.eslintignore",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@fern-api/configuration": "workspace:*",
    "@fern-api/docker-utils": "workspace:*",
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/ir-generator": "workspace:*",
    "@fern-api/ir-migrations": "workspace:*",
    "@fern-api/logging-execa": "workspace:*",
    "@fern-api/task-context": "workspace:*",
    "@fern-api/workspace-loader": "workspace:*",
    "@fern-fern/fiddle-sdk": "0.0.584",
    "@fern-fern/generator-exec-sdk": "^0.0.898",
    "chalk": "^5.3.0",
    "decompress": "^4.2.1",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@types/decompress": "^4.2.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.7.18",
    "@types/tmp": "^0.2.4",
    "depcheck": "^1.4.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^2.7.1",
    "typescript": "4.6.4"
  }
}
