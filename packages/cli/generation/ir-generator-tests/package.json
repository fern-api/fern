{
  "name": "@fern-api/ir-generator-tests",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "packages/cli/generation/ir-generator-tests"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "source": "./src/index.ts",
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "main": "lib/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": ["lib"],
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "compile:debug": "tsc --build --sourceMap",
    "depcheck": "depcheck",
    "test": "vitest --passWithNoTests --run",
    "test:debug": "pnpm run test --inspect --no-file-parallelism",
    "test:update": "vitest --passWithNoTests --run -u"
  },
  "dependencies": {
    "@fern-api/casings-generator": "workspace:*",
    "@fern-api/cli-source-resolver": "workspace:*",
    "@fern-api/configuration-loader": "workspace:*",
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/ir-generator": "workspace:*",
    "@fern-api/ir-sdk": "workspace:*",
    "@fern-api/ir-utils": "workspace:*",
    "@fern-api/project-loader": "workspace:*",
    "@fern-api/task-context": "workspace:*",
    "@fern-api/workspace-loader": "workspace:*"
  },
  "devDependencies": {
    "@fern-api/configs": "workspace:*",
    "@types/node": "18.15.3",
    "depcheck": "^1.4.7",
    "typescript": "5.9.3",
    "vitest": "^4.0.8"
  }
}
