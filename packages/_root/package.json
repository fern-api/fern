    "@fern-api/remote-workspace-runner": "workspace:*",
    "@fern-api/scripts": "workspace:*",
    "@fern-api/validator": "workspace:*",
    "@fern-api/workspace-configuration": "workspace:*",
    "@fern-api/workspace-loader": "workspace:*",
    "@fern-api/yaml-schema": "workspace:*",
    "@fern-typescript/client": "workspace:*",
    "@fern-typescript/client-v2": "workspace:*",
    "@fern-typescript/commons": "workspace:*",
    "@fern-typescript/commons-v2": "workspace:*",
    "@fern-typescript/declaration-handler": "workspace:*",
