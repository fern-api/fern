    "@types/decompress": "^4.2.7",
    "@types/esutils": "^2.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "18.15.3",
    "@types/url-join": "4.0.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.16.0",
    "vitest": "^2.1.8",
    "memfs": "^3.4.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "package-json-type": "^1.0.3",
    "prettier": "^3.4.2"
