{
  "name": "@fern-typescript/commons",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "generators/typescript/utils/commons"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "yarn compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../../.eslintignore --report-unused-disable-directives",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@fern-api/core-utils": "workspace:*",
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/logger": "workspace:*",
    "@fern-api/logging-execa": "workspace:*",
    "@fern-fern/ir-sdk": "0.0.3294",
    "@fern-typescript/fetcher": "workspace:*",
    "@fern-typescript/zurg": "workspace:*",
    "decompress": "^4.2.1",
    "esutils": "^2.0.3",
    "glob": "^8.0.3",
    "glob-promise": "^5.0.0",
    "immer": "^9.0.15",
    "js-yaml": "^4.1.0",
    "tmp-promise": "^3.0.3",
    "ts-morph": "^15.1.0",
    "typescript": "4.6.4",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/decompress": "^4.2.7",
    "@types/esutils": "^2.0.0",
    "@types/jest": "^29.0.3",
    "@types/js-yaml": "^4.0.8",
    "@types/node": "^18.7.18",
    "@types/url-join": "4.0.1",
    "depcheck": "^1.4.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "memfs": "^3.4.1",
    "organize-imports-cli": "^0.10.0",
    "package-json-type": "^1.0.3",
    "prettier": "^2.7.1"
  }
