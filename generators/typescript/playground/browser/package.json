{
  "name": "@fern-api/browser-playground",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "generators/typescript/playground/browser"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "module": "src/index.ts",
  "main": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "yarn compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../../.eslintignore --report-unused-disable-directives",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:style": "stylelint 'src/**/*.scss' --allow-empty-input --max-warnings 0",
    "lint:style:fix": "yarn lint:style --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck",
    "start": "yarn compile && vite",
    "build": "yarn compile && vite build",
    "preview": "yarn compile && vite preview"
  },
  "dependencies": {
    "form-data": "4.0.0",
    "js-base64": "^3.7.6",
    "node-fetch": "^3.3.2",
    "qs": "6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/node-fetch": "2.6.9",
    "@types/qs": "6.9.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/url-join": "4.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "depcheck": "^1.4.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^2.7.1",
    "sass": "^1.71.0",
    "stylelint": "^16.2.1",
    "typescript": "4.6.4",
    "vite": "^5.1.3",
    "vite-plugin-checker": "^0.6.4"
  }
}
