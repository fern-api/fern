{
  "name": "@fern-api/browser-playground",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "generators/typescript/playground/browser"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "source": "./src/index.ts",
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm compile && vite build",
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "compile:debug": "tsc --build --sourceMap",
    "depcheck": "depcheck",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../../shared/.prettierignore \"**\"",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-pattern=../../../../.eslintignore",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:style": "stylelint 'src/**/*.scss' --allow-empty-input --max-warnings 0",
    "lint:style:fix": "yarn lint:style --fix",
    "preview": "pnpm compile && vite preview",
    "start": "pnpm compile && vite",
    "test": "vitest --passWithNoTests --run",
    "test:debug": "pnpm run test --inspect --no-file-parallelism",
    "test:update": "vitest --passWithNoTests --run -u"
  },
  "dependencies": {
    "form-data": "4.0.1",
    "js-base64": "^3.7.6",
    "node-fetch": "2.7.0",
    "qs": "6.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@fern-api/configs": "workspace:*",
    "@types/node": "18.15.3",
    "@types/node-fetch": "2.6.9",
    "@types/qs": "6.9.15",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/url-join": "4.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "depcheck": "^1.4.7",
    "sass": "^1.72.0",
    "stylelint": "^16.2.1",
    "typescript": "5.7.2",
    "vite": "^5.4.12",
    "vite-plugin-checker": "^0.6.4",
    "vitest": "^2.1.9"
  }
}