{
  "name": "@fern-api/swift-sdk-runner",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "generators/swift/sdk-runner"
  },
  "sideEffects": false,
  "type": "module",
  "main": "lib/cli.js",
  "source": "src/cli.ts",
  "types": "lib/cli.d.ts",
  "files": ["lib"],
  "scripts": {
    "clean": "rm -rf ./lib && rm -rf ./dist && tsc --build --clean",
    "compile": "tsc --build",
    "compile:debug": "tsc --build --sourceMap",
    "depcheck": "depcheck",
    "dist:api": "pnpm compile && tsup ./src/api.ts --format cjs --sourcemap --external @boundaryml/baml && cp -R ../base/src/asIs dist && cp -R ../base/src/template dist",
    "dist:cli": "rm -rf ./lib && rm -rf ./dist && pnpm compile && tsup ./src/cli.ts --format cjs --sourcemap --external @boundaryml/baml && cp -R ../base/src/asIs dist && cp -R ../base/src/template dist",
    "publish:cli": "pnpm dist:cli && cd dist && yarn npm publish",
    "test": "vitest --passWithNoTests --run",
    "test:debug": "pnpm run test --inspect --no-file-parallelism",
    "test:update": "vitest --passWithNoTests --run -u"
  },
  "devDependencies": {
    "@fern-api/api-workspace-commons": "workspace:*",
    "@fern-api/cli-source-resolver": "workspace:*",
    "@fern-api/configs": "workspace:*",
    "@fern-api/core-utils": "workspace:*",
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/ir-generator": "workspace:*",
    "@fern-api/local-workspace-runner": "workspace:*",
    "@fern-api/project-loader": "workspace:*",
    "@fern-api/remote-workspace-runner": "workspace:*",
    "@fern-api/swift-sdk": "workspace:*",
    "@fern-api/task-context": "workspace:*",
    "@fern-api/workspace-loader": "workspace:*",
    "@types/node": "18.15.3",
    "@types/yargs": "^17.0.28",
    "depcheck": "^1.4.7",
    "tsup": "^8.5.0",
    "typescript": "5.9.2",
    "vitest": "^4.0.8",
    "yargs": "^17.4.1"
  }
}
