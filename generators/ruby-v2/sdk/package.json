{
  "name": "@fern-api/ruby-sdk",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "generators/ruby-v2/sdk"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "source": "./src/index.ts",
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "main": "lib/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "compile:debug": "tsc --build --sourceMap",
    "dockerTagLatest": "pnpm dist:cli && docker build -f ./Dockerfile -t fernapi/fern-ruby-sdk-v2:latest ../../..",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-pattern=../../../.eslintignore",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "depcheck": "depcheck",
    "dist:cli": "pnpm compile && tsup ./src/cli.ts --format cjs && cp -r ../base/src/asIs ./dist/asIs",
    "publish:cli": "pnpm dist:cli && cd dist && yarn npm publish",
    "test": "vitest --passWithNoTests --run",
    "test:debug": "pnpm run test --inspect --no-file-parallelism",
    "test:update": "vitest --passWithNoTests --run -u"
  },
  "devDependencies": {
    "@fern-api/ruby-model": "workspace:*",
    "@fern-api/ruby-ast": "workspace:*",
    "@fern-api/base-generator": "workspace:*",
    "@fern-api/configs": "workspace:*",
    "@fern-api/ruby-base": "workspace:*",
    "@fern-fern/generator-exec-sdk": "^0.0.1045",
    "@fern-fern/ir-sdk": "^58.2.0",
    "@types/node": "18.15.3",
    "depcheck": "^1.4.7",
    "tsup": "^8.5.0",
    "typescript": "5.8.3",
    "url-join": "^5.0.0",
    "vitest": "^2.1.9"
  }
}
