{
  "name": "@fern-api/java-base",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "generators/java-v2/base"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "vitest --passWithNoTests --run",
    "test:update": "vitest --passWithNoTests --run -u",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-pattern=../../../.eslintignore",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "depcheck": "depcheck",
    "dist:cli": "pnpm compile && tsup ./src/cli.ts --format cjs",
    "publish:cli": "pnpm dist:cli && cd dist && yarn npm publish",
    "dockerTagLatest": "pnpm dist:cli && docker build -f ./Dockerfile -t fernapi/fern-go-model:latest ../../.."
  },
  "devDependencies": {
    "@fern-api/base-generator": "workspace:*",
    "@fern-api/configs": "workspace:*",
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/java-ast": "workspace:*",
    "@fern-fern/ir-sdk": "^57.13.0",
    "@types/node": "18.15.3",
    "tsup": "^8.0.2",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.2",
    "vitest": "^2.1.9"
  }
}
