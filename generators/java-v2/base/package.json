{
  "name": "@fern-api/java-base",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "generators/java-v2/base"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports":{
    ".": {
      "development": "./src/index.ts",
      "source": "./src/index.ts",
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "compile:debug": "tsc --build --sourceMap",
    "test": "vitest --passWithNoTests --run",
    "test:debug": "pnpm run test --inspect --no-file-parallelism",
    "test:update": "vitest --passWithNoTests --run -u",
    "depcheck": "depcheck",
    "dist:cli": "pnpm compile && tsup ./src/cli.ts --format cjs --sourcemap",
    "publish:cli": "pnpm dist:cli && cd dist && yarn npm publish",
    "dockerTagLatest": "pnpm dist:cli && docker build -f ./Dockerfile -t fernapi/fern-go-model:latest ../../.."
  },
  "devDependencies": {
    "@fern-api/base-generator": "workspace:*",
    "@fern-api/configs": "workspace:*",
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/java-ast": "workspace:*",
    "@fern-fern/ir-sdk": "^58.2.0",
    "@types/node": "18.15.3",
    "depcheck": "^1.4.7",
    "typescript": "5.8.3",
    "vitest": "^2.1.9"
  }
}
