{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".npmrc",
    ".prettierrc.json",
    ".stylelintrc.json",
    "babel.config.json",
    "package.json",
    "shared/.prettierignore",
    "shared/stylelintrc.shared.json",
    "tsconfig.json",
    "turbo.json"
  ],
  "tasks": {
    "clean": {
      "cache": false
    },
    "compile": {
      "dependsOn": ["^compile"],
      "outputs": ["lib/**"],
      "inputs": ["src/**", "tests/**", "package.json", "tsconfig.json", "$TURBO_ROOT$/packages/configs/**"]
    },
    "compile:debug": {
      "dependsOn": ["^compile:debug"],
      "outputs": ["lib/**"],
      "inputs": ["src/**", "tests/**", "package.json", "tsconfig.json", "$TURBO_ROOT$/packages/configs/**"]
    },
    "depcheck": {
      "outputs": [],
      "inputs": ["src/**", "tests/**", "package.json", ".depcheckrc.json", "$TURBO_ROOT$/.depcheckrc.json"]
    },
    "lint:eslint": {
      "outputs": [],
      "inputs": ["src/**", "tests/**", "$TURBO_ROOT$/.eslintrc.js", "$TURBO_ROOT$/.eslintignore", "tsconfig.json"]
    },
    "lint:eslint:fix": {
      "outputs": [],
      "inputs": ["src/**", "tests/**", "$TURBO_ROOT$/.eslintrc.js", "$TURBO_ROOT$/.eslintignore", "tsconfig.json"]
    },
    "test": {
      "dependsOn": ["^compile", "compile"],
      "outputs": [],
      "inputs": [
        "$TURBO_ROOT$/packages/configs/**",
        "$TURBO_ROOT$/shared/vitest.config.ts",
        "$TURBO_ROOT$/test-definitions-openapi/**",
        "$TURBO_ROOT$/test-definitions/**",
        "$TURBO_ROOT$/tsconfig.eslint.json",
        "$TURBO_ROOT$/vitest.workspace.ts",
        "src/**",
        "tests/**",
        "tsconfig.json",
        "vitest.config.{js,mjs,mts,ts,cjs,cts}"
      ]
    },
    "test:debug": {
      "dependsOn": ["^compile:debug"],
      "outputs": [],
      "inputs": [
        "$TURBO_ROOT$/packages/configs/**",
        "$TURBO_ROOT$/shared/vitest.config.ts",
        "$TURBO_ROOT$/test-definitions-openapi/**",
        "$TURBO_ROOT$/test-definitions/**",
        "$TURBO_ROOT$/tsconfig.eslint.json",
        "$TURBO_ROOT$/vitest.workspace.ts",
        "src/**",
        "tests/**",
        "tsconfig.json",
        "vitest.config.{js,mjs,mts,ts,cjs,cts}"
      ]
    },
    "test:update": {
      "dependsOn": ["^compile", "compile"],
      "outputs": [],
      "inputs": [
        "$TURBO_ROOT$/packages/configs/**",
        "$TURBO_ROOT$/shared/vitest.config.ts",
        "$TURBO_ROOT$/test-definitions-openapi/**",
        "$TURBO_ROOT$/test-definitions/**",
        "$TURBO_ROOT$/tsconfig.eslint.json",
        "$TURBO_ROOT$/vitest.workspace.ts",
        "src/**",
        "tests/**",
        "tsconfig.json",
        "vitest.config.{js,mjs,mts,ts,cjs,cts}"
      ]
    },
    "dist:cli": {
      "outputs": ["dist/**"],
      "dependsOn": ["^compile"],
      "inputs": [
        "src/**",
        "tests/**",
        "package.json",
        "tsconfig.json",
        "build.mjs",
        "$TURBO_ROOT$/packages/configs/**",
        "$TURBO_ROOT$/shared/.prettierignore",
        "$TURBO_ROOT$/shared/stylelintrc.shared.json",
        "$TURBO_ROOT$/tsconfig.eslint.json"
      ]
    },
    "dist:cli:dev": {
      "outputs": ["dist/**"],
      "dependsOn": ["^compile"],
      "inputs": [
        "src/**",
        "tests/**",
        "package.json",
        "tsconfig.json",
        "build-utils.mjs",
        "build.dev.mjs",
        "$TURBO_ROOT$/shared/.prettierignore",
        "$TURBO_ROOT$/shared/stylelintrc.shared.json",
        "$TURBO_ROOT$/tsconfig.eslint.json",
        "$TURBO_ROOT$/packages/configs/**"
      ]
    },
    "dist:cli:local": {
      "outputs": ["dist/**"],
      "dependsOn": ["^compile"],
      "inputs": [
        "src/**",
        "tests/**",
        "package.json",
        "tsconfig.json",
        "build-utils.mjs",
        "build.local.mjs",
        "$TURBO_ROOT$/shared/.prettierignore",
        "$TURBO_ROOT$/shared/stylelintrc.shared.json",
        "$TURBO_ROOT$/tsconfig.eslint.json",
        "$TURBO_ROOT$/packages/configs/**"
      ]
    },
    "dist:cli:prod": {
      "outputs": ["dist/**"],
      "dependsOn": ["^compile"],
      "inputs": [
        "src/**",
        "tests/**",
        "package.json",
        "tsconfig.json",
        "build-utils.mjs",
        "build.prod.mjs",
        "$TURBO_ROOT$/shared/.prettierignore",
        "$TURBO_ROOT$/shared/stylelintrc.shared.json",
        "$TURBO_ROOT$/tsconfig.eslint.json",
        "$TURBO_ROOT$/packages/configs/**"
      ]
    },
    "dist": {
      "with": ["dist:cli", "dist:cli:dev", "dist:cli:local", "dist:cli:prod"],
      "cache": false,
      "outputs": ["dist/**"],
      "inputs": [
        "src/**",
        "tests/**",
        "package.json",
        "tsconfig.json",
        "$TURBO_ROOT$/shared/.prettierignore",
        "$TURBO_ROOT$/shared/stylelintrc.shared.json",
        "$TURBO_ROOT$/tsconfig.eslint.json",
        "$TURBO_ROOT$/packages/configs/**"
      ]
    },
    "dockerTagLatest": {
      "outputs": [],
      "dependsOn": ["dist:cli"],
      "inputs": ["dist/**", "Dockerfile", "Dockerfile.*"]
    },
    "podmanTagLatest": {
      "outputs": [],
      "dependsOn": ["dist:cli"],
      "inputs": [
        "Dockerfile",
        "$TURBO_ROOT$/shared/.prettierignore",
        "$TURBO_ROOT$/shared/stylelintrc.shared.json",
        "$TURBO_ROOT$/packages/configs/**",
        "src/**",
        "tests/**",
        "package.json",
        "tsconfig.json"
      ]
    }
  }
}
