{
  "name": "fern/empty",
  "version": "1.0.0",
  "description": "My Empty PHP Library",
  "keywords": [
    "empty",
    "seed",
    "api",
    "sdk"
  ],
  "license": [
    "LGPL-2.1-only",
    "GPL-3.0-or-later"
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "7.9.2",
    "guzzlehttp/promises": "2.0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "12.0.2",
    "friendsofphp/php-cs-fixer": "3.5.0",
    "phpstan/phpstan": "^1.12",
    "phpunit/php-timer": "8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Seed\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "\\Seed\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "build": [
      "@php -l src",
      "@php -l tests"
    ],
    "test": "phpunit",
    "analyze": "echo skip",
    "hello": "echo hello"
  }
}