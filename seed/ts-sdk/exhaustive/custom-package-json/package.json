{
    "name": "@fern/exhaustive",
    "version": "0.0.2",
    "private": false,
    "repository": "github:exhaustive/fern",
    "files": [
        "exampleFile",
        "dist",
        "types"
    ],
    "exports": {
        ".": {
            "node": "./dist/node/index.js",
            "import": "./dist/browser/esm/index.js",
            "require": "./dist/browser/cjs/index.js",
            "default": "./dist/browser/cjs/index.js",
            "types": "./types/index.d.ts"
        },
        "./package.json": "./package.json"
    },
    "types": "./types/index.d.ts",
    "scripts": {
        "format": "prettier . --write --ignore-unknown",
        "compile": "tsc",
        "bundle": "node build.js",
        "build": "pnpm compile && pnpm bundle",
        "test": "vitest",
        "test:unit": "vitest --project unit",
        "test:wire": "vitest --project wire"
    },
    "dependencies": {
        "lodash-es": "^4.17.21",
        "stream": "^0.0.2",
        "qs": "^6.11.2"
    },
    "devDependencies": {
        "webpack": "^5.97.1",
        "ts-loader": "^9.5.1",
        "vitest": "^3.2.4",
        "msw": "2.11.2",
        "@types/node": "^18.19.70",
        "esbuild": "~0.25.0",
        "prettier": "^3.4.2",
        "typescript": "~5.7.2"
    },
    "browser": {
        "fs": false,
        "os": false,
        "path": false,
        "stream": false,
        "command-exists": false,
        "execa": false
    },
    "packageManager": "pnpm@10.14.0",
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false
}
