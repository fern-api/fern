package com.fern.blog.api;

import com.fern.blog.api.blog.PostsServiceClient;
import com.fern.blog.api.dummy.DummyServiceClient;
import java.lang.String;
import java.util.Objects;
import java.util.concurrent.atomic.AtomicReference;
import java.util.function.Supplier;

public final class BlogApiClient {
  private final Supplier<PostsServiceClient> postsServiceClient;

  private final Supplier<DummyServiceClient> dummyServiceClient;

  public BlogApiClient(String url) {
    this.postsServiceClient = memoize(() -> PostsServiceClient.getClient(url));
    this.dummyServiceClient = memoize(() -> DummyServiceClient.getClient(url));
  }

  public final PostsServiceClient blog() {
    return this.postsServiceClient.get();
  }

  public final DummyServiceClient dummy() {
    return this.dummyServiceClient.get();
  }

  private static <T> Supplier<T> memoize(Supplier<T> delegate) {
    AtomicReference<T> value = new AtomicReference<>();
    return () ->  {
      T val = value.get();
      if (val == null) {
        val = value.updateAndGet(cur -> cur == null ? Objects.requireNonNull(delegate.get()) : cur);
      }
      return val;
    } ;
  }
}

